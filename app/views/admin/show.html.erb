<p id="notice"><%= notice %></p>

<h1>Category: <%= @category.name %></h1>

<table>
  <tr>
    <th>Name</th>
    <th>Url</th>
    <th>Description</th>
  </tr>

<% @urls.each do |url| %>
  <tr>
    <td><%= url.name %></td>
    <td><%= url.url %></td>
    <td><%= url.description %></td>
  </tr>
<% end %>
</table>

<br />
